{
    "version": 2,
    "functions": {
      "api/main": {
        "runtime": "python3.9"
      }
    },
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "20mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }
  